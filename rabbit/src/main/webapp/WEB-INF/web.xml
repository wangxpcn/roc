<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Rabbit</display-name>
  <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>com.roc.servlet.CreateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Query</servlet-name>
        <servlet-class>com.roc.servlet.QueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Query</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>com.roc.servlet.DelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Modify</servlet-name>
        <servlet-class>com.roc.servlet.ModifyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Modify</servlet-name>
        <url-pattern>/modify</url-pattern>
    </servlet-mapping>
</web-app>